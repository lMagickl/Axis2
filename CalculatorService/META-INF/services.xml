<service name="CalculatorService" scope="request">
    <description>Allow you to divide two numbers</description>
    <messageReceivers>
        <messageReceiver
                mep="http://www.w3.org/ns/wsdl/in-only"
                class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver"/>
        <messageReceiver
                mep="http://www.w3.org/ns/wsdl/in-out"
                class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </messageReceivers>
    <parameter name="ServiceClass">sample.axisversion.CalculatorService</parameter>
</service>